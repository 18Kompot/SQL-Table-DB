<%- include('./header')-%>
<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th scope="col">ID#</th>
        <th scope="col">Firstname</th>
        <th scope="col">Country</th>
        <th scope="col">Email</th>
        <th scope="col">Phone</th>
      </tr>
    </thead>
    <tbody>
      <% personarr.forEach((element,index) => { %>
      <tr>
        <th scope="row"><%= index+1 %></th>
        <td><%= element.Fname %></td>
        <td><%= element.Country %></td>
        <td><%= element.Email %></td>
        <td><%= element.Phone %></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
</div>
<%- include('./footer')-%>
